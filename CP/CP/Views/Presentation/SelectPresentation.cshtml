@model CP.Models.CreatedPresentationModel
@{
    ViewBag.Title = "SelectPresentation";
}
<h2>Имя:@Model.PresentationProperties.PresentationName</h2>
<h5>Описание:@Model.PresentationProperties.About</h5>
<h6>Дата создания:@Model.PresentationProperties.TimeOfCreate</h6>

<script src="@Url.Content("~/Content/jscripts/DrawScript.js")" type="text/javascript"></script>
<div>
    <canvas id="myCanvas" width="800" height="600">
    </canvas>
</div>
<script type="text/javascript">
function Play()
{
   var newModel = @(Html.Raw(Json.Encode(Model)));
   DrawJSONSlides(newModel);
}
</script>
<div>
    <input id="playPresentation" onclick="Play()" value="Просмотреть презентацию" type="button" />
</div>
